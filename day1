CREATE TABLE corporation (
	  corp_id SMALLINT
	, NAME VARCHAR(30)
	, PRIMARY KEY (corp_id)
);

CREATE TABLE corporation2 (
	  corp_id SMALLINT PRIMARY KEY 
	, NAME VARCHAR(30) 
); 

DROP TABLE corporation3;
